        this.jfbm = jfbm;
    }

    public void setJfr(String jfr) {
        this.jfr = jfr;
    }

    public void setJfrlxfs(String jfrlxfs) {
        this.jfrlxfs = jfrlxfs;
    }

    public void setGldsr(String[] gldsr) {
        this.gldsr = gldsr;
    }

    public void setYbagx(String[] ybagx) {

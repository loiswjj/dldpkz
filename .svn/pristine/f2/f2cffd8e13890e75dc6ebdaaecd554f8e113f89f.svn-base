package nju.software.baseframework.data.vo;

import java.util.Date;

/**
 * @description 开庭公告
 * Created by johnl on 2019/1/17.
 */
public class KtggModel {

    private int ajxh;
    private String ah;
    private String  ft; //法庭
    private Date ktrq; //开庭日期
    private String ajmc;
    private Date fbsj;

    public KtggModel(int ajxh, String ah, String ft, Date ktrq, String ajmc) {
        this.ajxh = ajxh;
        this.ah = ah;
        this.ft = ft;
        this.ktrq = ktrq;
        this.ajmc = ajmc;
    }

    public int getAjxh() {
        return ajxh;
    }

    public String getAh() {
        return ah;
    }

    public String getFt() {
        return ft;
    }

    public Date getKtrq() {
        return ktrq;
    }

    public String getAjmc() {
        return ajmc;
    }

    public Date getFbsj() {
        return fbsj;
    }

    public void setAjxh(int ajxh) {
        this.ajxh = ajxh;
    }

    public void setAh(String ah) {
        this.ah = ah;
    }

    public void setFt(String ft) {
        this.ft = ft;
    }

    public void setKtrq(Date ktrq) {
        this.ktrq = ktrq;
    }

    public void setAjmc(String ajmc) {
        this.ajmc = ajmc;
    }

    public void setFbsj(Date fbsj) {
        this.fbsj = fbsj;
    }
}
